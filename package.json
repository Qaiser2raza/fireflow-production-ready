{
  "name": "fireflow-restaurant-system",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "main": "electron-main.cjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "echo 'Linting not configured yet'",
    "test": "echo 'Tests not configured yet'",
    "server": "node --import tsx src/api/server.ts",
    "electron": "cross-env NODE_ENV=production node scripts/launch-electron.js",
    "test:integration": "node ./scripts/integration/delivery-test.js",
    "electron:dev": "concurrently \"npm run dev\" \"cross-env NODE_ENV=development node scripts/launch-electron.js\"",
    "electron:build": "npm run build && electron-builder",
    "migrate:pins": "tsx scripts/migrate-pins-to-bcrypt.ts",
    "db:seed": "prisma db seed",
    "db:reset": "prisma db seed",
    "db:reset-orders": "tsx scripts/reset_orders.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.0.0",
    "@supabase/supabase-js": "2",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "electron-store": "^11.0.2",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "idb": "^8.0.3",
    "jsqr": "^1.4.0",
    "lucide-react": "^0.554.0",
    "pg": "^8.11.3",
    "qrcode.react": "^4.2.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.11.0",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^22.19.3",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "electron": "^39.2.7",
    "electron-builder": "^24.6.4",
    "postcss": "^8.4.31",
    "prisma": "^6.0.0",
    "socket.io-client": "^4.7.2",
    "tailwindcss": "^4.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "build": {
    "appId": "com.fireflow.app",
    "productName": "Fireflow Restaurant System",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "electron-main.cjs",
      "preload.cjs",
      "server.cjs",
      "prisma/**/*",
      "package.json",
      "node_modules/**/*"
    ],
    "asar": true,
    "extraResources": [
      "prisma/**/*",
      ".env"
    ]
  }
}
